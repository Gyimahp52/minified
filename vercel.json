{
    "headers": [
      {
        "source": "/(.*)\\.(jpg|jpeg|gif|png|svg|ico|webp|css|js|woff2)$",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          },
          {
            "key": "Content-Encoding",
            "value": "br"
          }
        ]
      },
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
  }
  